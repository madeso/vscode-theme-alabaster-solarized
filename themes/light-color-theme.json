	"tokenColors": [
		{
			"name": "Compile-time constants",
			// numbers, booleans, true/false
			"scope": [
				"constant.numeric",
				"constant.character",
				"constant.keyword",
				"constant"
			],
			"settings": {
				"foreground": "#cb4b16"
			}
		},


		{
			"name": "String-like",
			// string, regexp, characters
			"scope": [
				//  - meta.preprocessor.include
				"string",
				"string.regexp",
				"constant.other.symbol"
			],
			"settings": {
				"foreground": "#859900"
			}
		},


		{
			"name": "Strings: Escape Sequences",
			"scope": "constant.character.escape",
			"settings": {
				"foreground": "#586e75"
			}
		},


		{
			"name": "Declarations",
			// top level definition of classes, functions, namespaces...
			"scope":
			[
				// - entity.name.function.call
				// - entity.name.function.member
				// - meta.function-call.(ts,js,c)
				"entity.name.function"

				// - entity.type.parameter
				// - meta.body.struct
				, "entity.name.type"

				// - entity.name.scope-resolition
				, "entity.name.namespace"
				, "entity.name.scope-resolution.function.definition"
				, "entity.global"
			],
			"settings": {
				"foreground": "#268bd2"
			}
		},


		{
			"name": "Comments",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				// optionally use emphasized here: 586e75
				// but I don't think it pops enough
				"foreground": "#000"
			}
		},


		
		{
			"name": "Punctuation",
			"scope": "punctuation",
			"settings": {
				"foreground": "#93a1a1"
			}
		},
		{
			"name": "Invalid",
			"scope": "invalid",
			"settings": {
				// "background": "#96000014",
				"foreground": "#dc322f"
			}
		},
		{
			"name": "Extra: Diff Range",
			"scope": [
				"meta.diff.range",
				"meta.diff.index",
				"meta.separator"
			],
			"settings": {
				"background": "#DDDDFF",
				"foreground": "#434343"
			}
		},
		{
			"name": "Extra: Diff From",
			"scope": "meta.diff.header.from-file",
			"settings": {
				"background": "#FFDDDD",
				"foreground": "#434343"
			}
		},
		{
			"name": "Extra: Diff To",
			"scope": "meta.diff.header.to-file",
			"settings": {
				"background": "#DDFFDD",
				"foreground": "#434343"
			}
		}
	],
}
